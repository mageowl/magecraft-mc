{
  "schemaVersion": 1,
  "id": "magecraft",
  "version": "${version}",

  "name": "Magecraft",
  "description": "Vanilla-style magic and owls for Minecraft.",
  "authors": [
    "MageOwl Studios"
  ],
  "contact": {
    "homepage": "https://mageowlstudios.com",
    "sources": "https://github.com/mageowl/magecraft-mc"
  },

  "license": "MIT License",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.mageowlstudios.magecraft.MagecraftMod"
    ],
    "client": [
      "net.mageowlstudios.magecraft.client.MagecraftModClient"
    ]
  },
  "mixins": [
    "magecraft.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric": "*",
    "minecraft": "~1.19",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
